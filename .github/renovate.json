{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>gopad/.github//renovate/preset"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "/^github.com\/uptrace\/bun\//"
      ],
      "groupName": "github.com-uptrace-bun deps"
    },
    {
      "matchPackageNames": [
        "/^github.com\/golangci\/golangci-lint\/v2\/"
      ],
      "labels": [
        "tools",
        "golangci"
      ]
    },
    {
      "matchPackageNames": [
        "/^github.com\/mgechev\/revive/"
      ],
      "labels": [
        "tools",
        "revive"
      ]
    },
    {
      "matchPackageNames": [
        "/^github.com\/golang\/mock/"
      ],
      "labels": [
        "tools",
        "mockgen"
      ]
    },
    {
      "matchPackageNames": [
        "/^github.com\/oapi-codegen\/oapi-codegen\/v2/"
      ],
      "labels": [
        "tools",
        "codegen"
      ]
    }
  ]
}
